[run]
source = .
parallel = 1
branch = 1

[report]
include = src/*, testing/*

[paths]
source = src/_pytest
  .tox/*/lib/python*/site-packages/_pytest/
  .tox\*\Lib\site-packages\pytest\
source_pytest_py = src/pytest.py
  .tox/*/lib/python*/site-packages/pytest.py
  .tox\*\Lib\site-packages\pytest.py
